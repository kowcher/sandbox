<script type="text/javascript">
jQuery( document ).ready(function() {
	var scs= [];
	var randomAoe = [];
	var facultyProfiles = [];
	jQuery.ajax({ 
		url: "/api/scs2.json",
		success: function( data ) {
			if (data.length > 0) { 
				scs = data;
				var scIndexes = [];

				while(scIndexes.length != 5){
					var random = Math.floor(Math.random() * data.length);    
					if(scIndexes.indexOf(random) == -1){
						scIndexes.push(random);
					}
				}

				scIndexes.forEach(element => {
console.log(jQuery("li.sc-tab[data-tid='"+scs[element].tid+"']"));
                    jQuery("li.sc-tab[data-tid='"+scs[element].tid+"']").attr("data-fac-count", scs[element].count);
                    jQuery("li.sc-tab[data-tid='"+scs[element].tid+"']").attr("data-sc-name", scs[element].name);
                });
                
                var thisTid = "10151";
                var thisCount = jQuery("li.sc-tab[data-tid='"+thisTid+"']").attr("data-fac-count");
                var thisScName = jQuery("li.sc-tab[data-tid='"+thisTid+"']").attr("data-sc-name");
                jQuery.ajax({
                    url: "/api/faculty-updated?limit=3&sc[]="+thisTid,
                    jsonp: "callback",
                    dataType: "jsonp",
                    success: function(data){
                        if(data){
                            item = '<div class="item"><div class="row">';
                            item += "<div class=\"col-md-12\"><h3 class=\"latest-aoe\">Latest Faculty Profiles in \"<span><a href=\"/faculty?filter[sc]=" + thisTid + "\">" + thisScName + "</a></span>\"</h3></div>";
                            item += '<div class="col-md-12 faculty-profiles">';															
            
                            item += '</div>'
                            item+= '</div></div>';
          
                            jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").empty();
                            jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").append(item);
            
                            facultyProfiles = data;
            
                            for (var i = 0; i < facultyProfiles.length; i++) {
                                var facultyProfile = facultyProfiles[i];
                                var facultyDiv = jQuery('<div></div>');
                                facultyDiv.addClass("col-md-3 col-sm-6 col-xs-12 faculty-profile-item");
                                var facultyDesignations = facultyProfile.designation.split(";");
                                var facultyDesignation = "";
                                if (facultyDesignations.length > 0) {
                                    facultyDesignation = facultyDesignations[0];
                                }
                                var staffPhoto = jQuery('<div></div>');
                                var facultyNameUrl = facultyProfile.node_title.split(' ').join('-');
                                staffPhoto.addClass("no-padding col-md-3");
                                staffPhoto.append("<a href=\"/faculty/profile/"+ facultyProfile.nid +"/"+ facultyNameUrl +"\"><img src=\"https://staffphoto.smu.edu.sg/s/"+facultyProfile.ntlogin+"/100x120\" class=\"faculty-photo img-responsive\"></a>");
                                facultyDiv.append(staffPhoto);
                                facultyDiv.append("<div class=\"col-md-9\"><h4 class=\"faculty-name\"><a href=\"/faculty/profile/"+facultyProfile.nid+"/"+ facultyNameUrl +"\">"+facultyProfile.node_title+"</a></h4><p>"+facultyDesignation+"</p></div>");		
                                jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").append(facultyDiv);
                            }
            
                            jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").append("<div class=\"col-md-3 col-sm-6 col-xs-12 faculty-profile-item text-center see-all-aoe\"><h4><a href=\"/faculty?filter[sc]="+thisTid+"\">See all " + thisCount + " faculties tackling this societal challenge</h4></div>");
                            jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").attr("data-loaded", 1);
                        }
                    }
                });                            
			
			} 
		}
    });

    jQuery("li.sc-tab").click(function() {
        var thisTid = jQuery(this).attr("data-tid");
        var thisCount = jQuery(this).attr("data-fac-count");
        var thisScName = jQuery(this).attr("data-sc-name");
        var dataLoaded = jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").attr("data-loaded");
        if (dataLoaded == 0) {
            jQuery.ajax({
                url: "/api/faculty-updated?limit=3&sc[]="+thisTid,
                jsonp: "callback",
                dataType: "jsonp",
                success: function(data){
                    if(data){
                        item = '<div class="item"><div class="row">';
                        item += "<div class=\"col-md-12\"><h3 class=\"latest-aoe\">Latest Faculty Profiles in \"<span><a href=\"/faculty?filter[sc]=" + thisTid + "\">" + thisScName + "</a></span>\"</h3></div>";
                        item += '<div class="col-md-12 faculty-profiles">';															

                        item += '</div>'
                        item+= '</div></div>';
                            jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").empty();
                        jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").append(item);

                        facultyProfiles = data;

                        for (var i = 0; i < facultyProfiles.length; i++) {
                            var facultyProfile = facultyProfiles[i];
                            var facultyDiv = jQuery('<div></div>');
                            facultyDiv.addClass("col-md-3 col-sm-6 col-xs-12 faculty-profile-item");
                            var facultyDesignations = facultyProfile.designation.split(";");
                            var facultyDesignation = "";
                            if (facultyDesignations.length > 0) {
                                facultyDesignation = facultyDesignations[0];
                            }
                            var staffPhoto = jQuery('<div></div>');
                            var facultyNameUrl = facultyProfile.node_title.split(' ').join('-');
                            staffPhoto.addClass("no-padding col-md-3");
                            staffPhoto.append("<a href=\"/faculty/profile/"+ facultyProfile.nid +"/"+ facultyNameUrl +"\"><img src=\"https://staffphoto.smu.edu.sg/s/"+facultyProfile.ntlogin+"/100x120\" class=\"faculty-photo img-responsive\"></a>");
                            facultyDiv.append(staffPhoto);
                            facultyDiv.append("<div class=\"col-md-9\"><h4 class=\"faculty-name\"><a href=\"/faculty/profile/"+facultyProfile.nid+"/"+ facultyNameUrl +"\">"+facultyProfile.node_title+"</a></h4><p>"+facultyDesignation+"</p></div>");		
                            jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").append(facultyDiv);
                        }

                        jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").append("<div class=\"col-md-3 col-sm-6 col-xs-12 faculty-profile-item text-center see-all-aoe\"><h4><a href=\"/faculty?filter[sc]="+thisTid+"\">See all " + thisCount + " faculties tackling this societal challenge</h4></div>");
                        jQuery(jQuery("li.sc-tab[data-tid='"+thisTid+"'] a").attr("href")).find("div.fac-profile-row").attr("data-loaded", 1);
                    }
                }
            });        
        }
    });
});
</script> </div>
</div></div>
